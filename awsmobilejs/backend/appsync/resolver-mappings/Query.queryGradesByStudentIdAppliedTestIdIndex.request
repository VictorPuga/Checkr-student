{
  "version": "2017-02-28",
  "operation": "Query",
  "query": {
    "expression": "#appliedTestId = :appliedTestId",
    "expressionNames": {
      "#appliedTestId": "appliedTestId",
    },
    "expressionValues": {
      ":appliedTestId": $util.dynamodb.toDynamoDBJson($ctx.args.appliedTestId),
